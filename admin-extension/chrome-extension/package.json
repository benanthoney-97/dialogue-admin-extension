{
  "name": "Dialogue",
  "displayName": "Dialogue",
  "version": "0.1.3",
  "description": "The contextual intelligence layer for your website.",
  "author": "Dialogue",
  "scripts": {
    "predev": "node ./scripts/generate-admin-config.js",
