    "dev": "PLASMO_PORT=4175 plasmo dev",
    "build": "plasmo build",
    "sync-admin-script": "true",
    "package": "plasmo package"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.78.0",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "lucide-react": "^0.561.0",
    "markdown-to-jsx": "^9.4.2",
    "motion": "^12.23.26",
    "openai": "^6.15.0",
    "pg": "^8.16.3",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rss-parser": "^3.13.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "3.4.1",
    "use-stick-to-bottom": "^1.1.1",
    "yt-dlp-exec": "^1.0.2"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.3.0",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@tailwindcss/typography": "^0.5.19",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.23",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
"https://*/*"
    ],
    "permissions": [
      "sidePanel",
      "storage",
      "scripting",
      "activeTab"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "static/player.js"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "action": {
      "default_title": "Dialogue"
    }
  }
